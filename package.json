{
  "name": "nestjs-postgres-swagger-starterkit",
  "version": "0.1.0",
  "description": "",
  "author": "Stepan Soroka",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "lint": "tslint 'src/**/*.ts'",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts --timeout 15000 --exit",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:hmr": "node dist/server",
    "prod:clear": "rm -rf dist",
    "prod:build": "npm run prod:clear && tsc && npm run config:build",
    "prod:ci-build": "npm run prod:build",
    "prod:start": "npm run prod:build && node ./dist/src/main.js",
    "prod:pginit": "npm run migrate-run; npm run fillpg:proddata; npm run insert-software",
    "config:build": "cp config/* dist/config/ && cp -r public dist/public && cp -r node_modules dist/node_modules",
    "db:seeds:fill:dev": "ts-node ./scripts/filldb.ts --dev",
    "db:seeds:fill:prod": "ts-node ./scripts/filldb.ts --prod",
    "typeorm": "ts-node ./node_modules/.bin/typeorm",
    "migrate-run": "npm run typeorm -- migration:run",
    "migrate-revert": "npm run typeorm -- migration:revert",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/jwt": "^0.1.3",
    "@nestjs/swagger": "^2.2.0",
    "@nestjs/typeorm": "^5.0.2",
    "chalk": "^2.4.1",
    "config": "^2.0.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "pg": "^7.4.3",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.7",
    "typescript": "^2.6.2",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.0.0",
    "@types/chai": "^4.1.6",
    "@types/express": "^4.0.39",
    "@types/node": "^9.3.0",
    "@types/mocha": "^5.2.5",
    "@types/supertest": "^2.0.4",
    "nodemon": "^1.14.1",
    "ts-loader": "^4.1.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.3.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  }
}
